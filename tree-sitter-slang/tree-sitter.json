{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "slang",
      "camelcase": "Slang",
      "title": "Slang",
      "scope": "source.slang",
      "file-types": [
        "slang"
      ],
      "injection-regex": "^slang$",
      "class-name": "TreeSitterSlang",
      "highlights": "queries/highlights.scm",
      "locals": "queries/locals.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Slang grammar for tree-sitter",
    "authors": [
      {
        "name": "Jonathan Kohlhs"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-slang"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}